<svg
  aria-label="CYB3RL4NG logo"
  xmlns="http://www.w3.org/2000/svg"
  width="100%"
  height="100%"
  viewBox="0 0 470 420"
  class="svg-logo"
>
  <defs>
    <style type="text/css">
      @font-face {
        font-family: "SuperLegendBoy";
        src: url("/fonts/super-legend-boy/woff/SuperLegendBoy.woff");
        src:
          url("/fonts/super-legend-boy/woff/SuperLegendBoy.woff") format("woff"),
          url("/fonts/super-legend-boy/woff/SuperLegendBoy.woff2")
            format("woff2"),
          url("/fonts/super-legend-boy/otf/SuperLegendBoy.otf")
            format("opentype"),
          url("/fonts/super-legend-boy/ttf/SuperLegendBoy.ttf")
            format("truetype");
      }
    </style>

    <filter id="text-shadow">
      <feDropShadow
        dx="-6"
        dy="0"
        stdDeviation="0"
        flood-color="currentColor"
        flood-opacity="1.0"></feDropShadow>
      <feDropShadow
        dx="6"
        dy="0"
        stdDeviation="0"
        flood-color="currentColor"
        flood-opacity="1.0"></feDropShadow>
      <feDropShadow
        dx="0"
        dy="-6"
        stdDeviation="0"
        flood-color="currentColor"
        flood-opacity="1.0"></feDropShadow>
      <feDropShadow
        dx="0"
        dy="6"
        stdDeviation="0"
        flood-color="currentColor"
        flood-opacity="1.0"></feDropShadow>
    </filter>
  </defs>
  <g
    stroke="currentColor"
    stroke-width="20"
    fill="none"
    transform="translate(25)"
  >
    <ellipse cx="210" cy="210" rx="200" ry="200"></ellipse>
    <path d="M 210 0 v 420"></path>
    <path d="M 0 210 h 420"></path>
    <path d="M 59,90 a 260,260 0 0,0 302,0"></path>
    <path d="M 361,330 a 260,260 0 0,0 -302,0"></path>
    <path d="M 210,10 a 260,260 0 0,0 0,400"></path>
    <path d="M 210,410 a 260,260 0 0,0 0,-400"></path>
  </g>
  <text
    x="235"
    y="175"
    text-anchor="middle"
    dominant-baseline="central"
    fill="#ca3148"
    stroke="none"
    font-size="52"
    style="font-family: SuperLegendBoy; filter:url(#text-shadow);"
    transform="scale(1, 1.2)"
  >
    <!-- scale(1, 1.2) stretches the text vertically -->
    CYB3RL4NG
  </text>
</svg>

<style>
  .svg-logo {
    margin-right: 10px;
    color: var(--logo-text-bg);
    display: flex;
    align-items: center;

    g {
      color: var(--logo-path);
      transition-property: color;
    }

    text {
      fill: var(--logo-text);
    }
  }
</style>
